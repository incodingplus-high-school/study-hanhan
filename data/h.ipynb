    "def hanoi(place,a,c,b):\n",
    "    if place == 1:\n",
    "        print(f'1번 원반을 {a}에서 {c}로 이동')\n",
    "    else:\n",
    "        hanoi(place-1,a,b,c)\n",
    "        print(f'{place}번 원반을 {a}에서 {c}로 이동')\n",
    "        hanoi(place-1,b,c,a)\n",
    "hanoi(3,'a','b','c')"
   ]
  }
